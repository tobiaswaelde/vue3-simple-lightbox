{
  "trailingComma": "all",
  "semi": true,
  "endOfLine": "auto",
  "printWidth": 120,
  "singleQuote": true,
  "vueIndentScriptAndStyle": true,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "singleQuote": true,
        "vueIndentScriptAndStyle": false,
        "parser": "vue"
      }
    },
    {
      "files": "*.html",
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": "*.ts",
      "options": {
        "singleQuote": true,
        "parser": "typescript"
      }
    }
  ]
}